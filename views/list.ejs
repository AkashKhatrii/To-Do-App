
  <%- include("header") -%>

    <div class="box" id="heading">
      <h1><%= listType%></h1>
    </div>

    <div class="box">
      <% for(var i = 0;i < newItems.length;i++){ %>
      <div class="item">
        <input type="checkbox" />
        <p><%= newItems[i]%></p>
      </div>
      <% } %>

      <form class="item" action="/" method="POST">
        <input
          name="newItem"
          type="text"
          placeholder="New Item"
          autocomplete="off"
        />
        <button type="submit" name="list" value=<%= listType%>>+</button>
      </form>
    </div>
  
    <%- include("footer") -%>